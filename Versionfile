# Examples
#
# "0.50.x" => { :branch => "master" }
# "0.40.x" => { :tag => "v1.0.0", :version => "1.0.0" }


"0.60.x" => { :tag => "v0.60.0", :version => "0.60.0" }
"0.70.x" => { :branch => "master" }